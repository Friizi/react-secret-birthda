;\nimport { RankingProvider, useRanking } from \"./RankingContext\";\nimport Menu from \"./Menu\"; // Importez le composant Menu\nimport \"./PublicPage.css\";\nimport secretStoryLogo from \"./images/Secret_Story_Logo.png\";\nimport { Tag } from \"primereact/tag\";\n\nconst PublicPage = () => {\n  const { rankings, loading } = useRanking();\n\n  // Fonction pour générer l'URL de l'image du participant en fonction de son nom\n  const getParticipantImageUrl = (participantName) => {\n    // Convertir le nom du participant en minuscules et remplacer les espaces par des tirets bas\n    const imageName = participantName.toLowerCase().replace(/ /g, \"_\") + \".png\";\n\n    // Retourner l'URL complète de l'image en utilisant le chemin du répertoire public\n    return `/images/${imageName}`;\n  };\n\n  return (\n    <div className=\"public-page\">\n      <img\n        src={secretStoryLogo}\n        alt=\"Logo Secret Story\"\n        className=\"logo\"\n        style={{ width: \"300px\", height: \"auto\" }}\n      />\n      <Card className=\"card\">\n        <Menu />\n        {loading ? (\n          <div>Loading...</div>\n        ) : (\n          <DataTable\n            value={rankings}\n            className=\"datatable responsive-datatable\"\n            stripedRows\n          >\n            <Column\n              header=\"#\"\n              headerStyle={{ width: \"3rem\" }}\n              body={(data, options) => (\n                <span className=\"circle-pink\">{options.rowIndex + 1}</span>\n              )}\n            />\n            <Column\n              field=\"participant\"\n              header=\"PARTICIPANT\"\n              body={(rowData) => (\n                <div className=\"participant-container\">\n                  <img\n                    src={getParticipantImageUrl(rowData.nom)}\n                    className=\"participant-image\"\n                  />\n                  <div className=\"participant-name\">\n                    {rowData.nom.toUpperCase()}\n                  </div>\n                </div>\n              )}\n            />\n            <Column\n              field=\"Secret\"\n              header=\"SECRET\"\n              body={(rowData) => (rowData.find !== 0 ? rowData.secret : \"\")}\n              className=\"secret-text\"\n            />\n            <Column\n              field=\"cagnotte\"\n              header=\"CAGNOTTE\"\n              body={(rowData) => (\n                <Tag\n                  value={rowData.cagnotte.toLocaleString() + \" €\"}\n                  severity={\n                    rowData.cagnotte === 0\n                      ? \"danger\"\n                      : rowData.cagnotte > 0 && rowData.cagnotte < 10000\n                      ? \"warning\"\n                      : \"success\"\n                  }\n                ></Tag>\n              )}\n            />\n          </DataTable>\n        )}\n      </Card>\n      <Button\n        label=\"Actualiser le classement\"\n        icon=\"pi pi-refresh\"\n        className={classNames(\"refresh-button\")}\n      />\n    </div>\n  );\n};\n\nconst App = () => {\n  return (\n    <RankingProvider>\n      <PublicPage />\n    </RankingProvider>\n  );\n};\n\nexport default App;\n","/Users/friizi/Documents/Project/node/secret-birthday/react/src/components/PrivatePage.js",[],[],"/Users/friizi/Documents/Project/node/secret-birthday/react/src/components/RankingContext.js",[],[],"/Users/friizi/Documents/Project/node/secret-birthday/react/src/components/Menu.js",[],[],"/Users/friizi/Documents/Project/node/secret-birthday/react/src/components/ParticipantPage.js",[],[],"/Users/friizi/Documents/Project/node/secret-birthday/react/src/components/SecretPage.js",[],[],"/Users/friizi/Documents/Project/node/secret-birthday/react/src/components/AdminPage.js",[],[],{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","severity":1,"message":"59","line":55,"column":19,"nodeType":"60","endLine":58,"endColumn":21},"no-new-object",["61"],"jsx-a11y/alt-text","i